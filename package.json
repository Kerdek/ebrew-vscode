{
    "name": "ebrew-vscode",
    "publisher": "4984",
    "displayName": "ebrew-vscode",
    "description": "Ebrew VSCode Support",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
